    }

    .cardTestimonial {
        margin: 0px; /* Adjust the padding value as needed */
    }

    .solution {
        margin-left: 118px;
    }

    ul.custom-bullets {
        list-style: none; /* Remove default bullets */
        padding-left: 2rem; /* Add space for custom bullets */
    }

        ul.custom-bullets li {
            position: relative;
            padding-left: 2rem; /* Space for the bullet image */
            font-family: 'Poppins', sans-serif;
            font-size: 16px;
            font-weight: 400;
            line-height: 24px;
            letter-spacing: 0.1em;
            text-align: left;
            color: #343434;
        }

            ul.custom-bullets li::before {
                content: "";
                position: absolute;
                left: 0;
                top: 50%;
                transform: translateY(-50%);
                width: 1.5rem; /* Size of the bullet image */
                height: 1.5rem; /* Size of the bullet image */
                background-image: url('/Images/PawPrintIcon32px.png'); /* Replace with your image URL */
                background-size: cover; /* Ensure image covers the space */
                background-repeat: no-repeat; /* Prevent repetition of the image */
            }

    .custom-bullets {
        list-style: none; /* Remove default bullets */
        padding-left: 1.5rem; /* Add space for custom bullets */
    }

        .custom-bullets li {
            position: relative;
            padding-left: 2rem; /* Space for the bullet image */
            margin-bottom: 0.5rem; /* Add space between list items */
        }

            .custom-bullets li::before {
                content: "";
                position: absolute;
                left: 0;
                top: 50%;
                transform: translateY(-50%);
                width: 1.5rem; /* Size of the bullet image */
                height: 1.5rem; /* Size of the bullet image */
                background-image: url('https://via.placeholder.com/24'); /* Replace with your image URL */
                background-size: cover; /* Ensure image covers the space */
                background-repeat: no-repeat; /* Prevent repetition of the image */
            }

    .list-container {
        max-height: 500px; /* Set a maximum height for scrolling */
        overflow-y: auto; /* Enable vertical scrolling */
        padding: 20px;
        border: 1px solid #ddd;
        border-radius: 5px;
