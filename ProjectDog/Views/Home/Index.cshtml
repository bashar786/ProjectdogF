<script>
    let currentIndex = 0;
    const testimonials = document.querySelectorAll('.cardTestimonial');
    const testimonialsCount = testimonials.length;

    function showTestimonials(startIndex) {
        // Hide all testimonials
        testimonials.forEach(testimonial => {
            testimonial.style.display = 'none';
        });

        // Determine end index
        let endIndex = startIndex + 3;
        if (endIndex > testimonialsCount) {
            endIndex = testimonialsCount;
        }

        // Show testimonials within range
        for (let i = startIndex; i < endIndex; i++) {
            testimonials[i].style.display = 'block';
        }
    }

    function nextTestimonials() {
        currentIndex += 3;
        if (currentIndex >= testimonialsCount) {
            currentIndex = 0;
        }
        showTestimonials(currentIndex);
    }

    function prevTestimonials() {
        currentIndex -= 2;
        if (currentIndex < 0) {
            currentIndex = testimonialsCount - 2;
        }
        showTestimonials(currentIndex);
    }

    // Auto-play testimonials
    setInterval(nextTestimonials, 5000); // Change 5000 to adjust the interval (in milliseconds)

    // Initial display
    showTestimonials(currentIndex);
</script>
