    <header class="site-navbar js-sticky-header site-navbar-target" role="banner">

        <div class="container">
            <div class="row align-items-center position-relative">


                <div class="site-logo">
                    <a class="navbar-brand" href="/home">
                        <img src="~/Images/ProjectDog_LOGO.png" width="150" height="100" alt="">
                    </a>
                </div>

                <div class="col-12">
                    <nav class="site-navigation text-right ml-auto " role="navigation">

                        <ul class="site-menu main-menu js-clone-nav ml-auto d-none d-lg-block">
                            <li><a href="@Url.Action("Video","Home")" class="nav-link">Video Education</a></li>
                            @* <li><a href="#pricing-section" class="nav-link">Pricing</a></li> *@
                            @* <li><a href="#testimonials-section" class="nav-link">Testimonial</a>< /li> *@
                            <li><a href="@Url.Action("ContactUs","Home")" class="nav-link">Contact Us</a></li>

                            <li><a href="@Url.Action("Login","Home")" class="nav-link">| &nbsp; <span class="icon-user"></span></a></li>
                            @* <li><a href="#testimonials-section" class="nav-link">Testimonials</a></li> *@
                        </ul>
                    </nav>

                </div>

